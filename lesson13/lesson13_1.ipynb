   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# 建立一些資料\n",
    "x = np.linspace(0, 10, 100)\n",
    "y = np.sin(x)\n",
    "\n",
    "# 繪製圖表\n",
    "plt.plot(x, y)\n",
    "\n",
    "# 加入標題和標籤\n",
    "plt.title(\"Simple Sine Wave\")\n",
    "plt.xlabel(\"X-axis\")\n",
    "plt.ylabel(\"Y-axis\")\n",
    "\n",
    "# 顯示圖表\n",
    "plt.show()"
   ]
  }
